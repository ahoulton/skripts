on connect:
    if {whitelist} is true:
        player doesn't have permission "whitelist"
        kick player due to "&6fatalworlds &8- &cYou are not whitelisted, contact an administator if you expect this is an error."

command /whitelist <boolean>:
    permission: op
    trigger:
        set {whitelist} to arg-1
        send "&7Whitelist: &e%{whitelist}%" to sender